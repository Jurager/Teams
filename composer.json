{
    "name": "jurager/teams",
    "description": "Laravel Teams and Roles using Jetstream",
    "keywords": ["laravel", "teams", "auth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Yuri Gerassimov",
            "email": "jurager01@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/bus": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/mail": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/queue": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/sanctum": "^2.0|^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Jurager\\Teams\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "stubs/app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jurager\\Teams\\TeamsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
